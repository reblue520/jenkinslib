#!groovy

//Pipeline
pipeline{

	agent {
		node {	label "master" }

	}

	stages{

		stage("mavenBuild"){
			steps{
				script{
					mvnHome = tool "M2"
					sh {
						${mvnHome}/bin/mvn -v
					}
				}
			}
		}

		stage("antBuild"){
			steps{
				script{
					antHome = tool "ANT"
					sh {
						${antHome}/bin/ant -v
					}
				}
			}
		}
	}
}
