#!groovy

String workspace = "/data/www/.jenkins/workspace"

//Pipeline
pipeline{

	agent {
		node {	label "main"	// 运行代码的节点或者标签
			customWorkspace "${workspace}" // 指顶运行工作目录
		
		}
	}
    
    stages{

        stage("build"){
            steps{
                script{
                   mvnHome = tools "M2"
		   sh {${mvnHome}/bin/mvn -v
		}
	    }
	}
    }
}
}
