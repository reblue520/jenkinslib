#!groovy

//Pipeline
pipeline{

	agent {
		node {	label "master" }
	
	}
    
	    stages{

		stage("build"){
		    steps{
			script{
			   mvnHome = tool "M2"
			   sh {${mvnHome}/bin/mvn -v
			}
		    }
		}
			
		stage("antbuild"){
			steps{
				script{
					antHome = tool "ANT"
					sh {
						${antHome}/bin/ant -v
					}
				}
			}
		}
	    }
	}
}
